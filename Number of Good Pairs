class Solution {
public:
    int numIdenticalPairs(vector<int>& nums) {
        int count = 0;
        for(int i = 0;i<nums.size();i++)
        {
            for(int y = i+1;y<nums.size();y++)
            {
                if(nums[i] == nums[y])
                    count++;
            }
        }
        return count;
        
    }
};
