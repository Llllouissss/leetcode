class Solution {
public:
    int numJewelsInStones(string jewels, string stones) {
        int count = 0;
        for(int i = 0;i < jewels.length();i++)
        {
            for(int y = 0;y <stones.length(); y ++)
            {
                if(jewels[i] == stones[y])
                    count++;
            }
        }
        return count;
        
    }
};
