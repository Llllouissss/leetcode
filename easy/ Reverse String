class Solution {
public:
    void reverseString(vector<char>& s) {
        int z[100000]= {};
        for(int i = 0;i<s.size();i++)
        {
            cout<<s[i];
        }
        for(int i = 0 ; i<s.size(); i++)
        {
            z[i] = s[s.size() - 1 -i] - '0';
        }
        for(int i = 0; i<s.size();i++)
        {
            s[i] = z[i] + '0';
        }
        
        
    }
};
